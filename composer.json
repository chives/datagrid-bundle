{
    "name": "fsi/datagrid-bundle",
    "type": "symfony-bundle",
    "description": "FSi Datagrid Bundle",
    "keywords": ["fsi", "symfony", "datagrid", "datagrid-bundle", "bundle"],
    "license": "MIT",
    "authors": [
        {
            "name": "Norbert Orzechowicz",
            "email": "norbert@orzechowicz.pl"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:chives/datagrid.git"
        }
    ],
    "require": {
        "php": ">=7.1",
        "twig/twig": "^1.28|^2.0",
        "symfony/property-access": "^2.3|^3.0|^4.0",
        "symfony/framework-bundle" : "^2.4|^3.0|^4.0",
        "symfony/options-resolver": "^2.6|^3.0|^4.0",
        "fsi/datagrid": "dev-dev-redesign as 2.0.0"
    },
    "require-dev": {
        "symfony/form" : "^2.4|^3.0|^4.0",
        "symfony/validator" : "^2.4|^3.0|^4.0",
        "symfony/translation" : "^2.4|^3.0|^4.0",
        "symfony/doctrine-bridge": "^2.2|^3.0|^4.0",
        "symfony/phpunit-bridge": "^3.3|^4.0",
        "symfony/security-csrf": "^2.4|^3.0|^4.0",
        "symfony/twig-bridge" : "^2.2.6|^3.0|^4.0",
        "symfony/yaml" : "^2.4|^3.0|^4.0",
        "phpunit/phpunit": "^6.5"
    },
    "autoload": {
        "psr-4": { "FSi\\Bundle\\DataGridBundle\\": "" }
    },
    "suggest": {
        "phpoffice/phpexcel" : "Allows exporting data to xls format."
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
}
